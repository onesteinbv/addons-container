<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="rule_curq_users_cud" model="ir.rule">
        <field name="name">rule_curq_users_cud</field>
        <field name="model_id" ref="base.model_res_users" />
        <field
            name="groups"
            eval="[(4, ref('container_accessibility.group_curq'))]"
        />
        <field
            name="domain_force"
            eval="[('id', 'not in', [ref('base.user_admin'), ref('base.user_root')])]"
        />
        <field name="perm_read" eval="0" />
        <field name="perm_create" eval="0" />
        <field name="perm_write" eval="1" />
        <field name="perm_unlink" eval="1" />
    </record>
</odoo>
