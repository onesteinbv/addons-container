<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Chart Template -->
    <record id="l10nnl_rgs_chart_template" model="account.chart.template">
        <field name="name">Nederlands Referentie Grootboekschema</field>
        <field name="cash_account_code_prefix">100101</field>
        <field name="bank_account_code_prefix">100201</field>
        <field name="transfer_account_code_prefix">100301</field>
        <field name="code_digits">7</field>
        <field name="use_anglo_saxon" eval="True"/>
        <field name="spoken_languages" eval="'nl_NL;en_GB;'"/>
        <field name="currency_id" ref="base.EUR"/>
        <field name="country_id" ref="base.nl"/>
    </record>
</odoo>
