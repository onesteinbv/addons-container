#. module: account
#: model:mail.template,body_html:account.email_template_edi_invoice
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Dear\n"
"        <t t-if=\"object.partner_id.parent_id\">\n"
"            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t> (<t t-out=\"object.partner_id.parent_id.name or ''\">Azure Interior</t>),\n"
"        </t>\n"
"        <t t-else=\"\">\n"
"            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t>,\n"
"        </t>\n"
"        <br><br>\n"
"        Please find attached the invoice of:\n"
"        <br><br>\n"
"        Company name:<span style=\"font-weight:bold;\" t-out=\"object.company_id.name or ''\">YourCompany</span>.\n"
"        <br><br>\n"
"        <t t-if=\"object.name\">\n"
"            Invoice number: <span style=\"font-weight:bold;\" t-out=\"object.name or ''\">INV/2021/05/0005</span><br><br>\n"
"        </t>\n"
"        <t t-if=\"object.invoice_date_due\">\n"
"             Due date:<span style=\"font-weight:bold;\" t-out=\"object.invoice_date_due or ''\">12/05/2024</span><br><br>\n"
"        </t>\n"
"        Amount: <span style=\"font-weight:bold;\" t-out=\"format_amount(object.amount_total, object.currency_id) or ''\">$ 143,750.00</span>\n"
"        <br><br><br>\n"
"        <t t-if=\"object.payment_state in ('paid', 'in_payment')\">\n"
"            This invoice is already paid.\n"
"        </t>\n"
"        <t t-else=\"\">\n"
"            Please transfer payment as soon as possible\n"
"            <t t-if=\"object.company_id.bank_ids and object.company_id.bank_ids[0].acc_number\">\n"
"                to <span style=\"font-weight:bold;\" t-out=\"object.company_id.bank_ids[0].acc_number or ''\">329237899</span>\n"
"            </t>\n"
"            <t t-if=\"object.name\">\n"
"                , stating invoice number <span style=\"font-weight:bold;\" t-out=\"object.name or ''\">INV/2021/05/0005</span>.\n"
"            </t>\n"
"        </t>\n"
"        <br><br>\n"
"        Should you have any questions, please do not hesitate to contact us.\n"
"        <t t-if=\"not is_html_empty(object.invoice_user_id.signature)\">\n"
"            <br><br>\n"
"            <t t-out=\"object.invoice_user_id.signature or ''\"><br>Mitchell Admin</t>\n"
"        </t>\n"
"        <br><br>\n"
"        Kind regards,\n"
"        <br>\n"
"        <t t-out=\"object.invoice_user_id.name or ''\">Mitchell Admin</t>\n"
"    </p>\n"
"</div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Beste\n"
"        <t t-if=\"object.partner_id.parent_id\">\n"
"            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t> (<t t-out=\"object.partner_id.parent_id.name or ''\">Azure Interior</t>),\n"
"        </t>\n"
"        <t t-else=\"\">\n"
"            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t>,\n"
"        </t>\n"
"        <br><br>\n"
"        In de bijlage treft u de factuur van:\n"
"        <br><br>\n"
"        Bedrijfsnaam:<span style=\"font-weight:bold;\" t-out=\"object.company_id.name or ''\">YourCompany</span>.\n"
"        <br><br>\n"
"        <t t-if=\"object.name\">\n"
"            Factuurnummer: <span style=\"font-weight:bold;\" t-out=\"object.name or ''\">INV/2021/05/0005</span><br><br>\n"
"        </t>\n"
"        <t t-if=\"object.invoice_date_due\">\n"
"             Vervaldatum:<span style=\"font-weight:bold;\" t-out=\"object.invoice_date_due or ''\">12/05/2024</span><br><br>\n"
"        </t>\n"
"        Bedrag: <span style=\"font-weight:bold;\" t-out=\"format_amount(object.amount_total, object.currency_id) or ''\">$ 143,750.00</span>\n"
"        <br><br><br>\n"
"        <t t-if=\"object.payment_state in ('paid', 'in_payment')\">\n"
"            Deze factuur is reeds betaald.\n"
"        </t>\n"
"        <t t-else=\"\">\n"
"            Graag de betaling zo spoedig mogelijk over te maken \n"
"            <t t-if=\"object.company_id.bank_ids and object.company_id.bank_ids[0].acc_number\">\n"
"                op <span style=\"font-weight:bold;\" t-out=\"object.company_id.bank_ids[0].acc_number or ''\">329237899</span>\n"
"            </t>\n"
"            <t t-if=\"object.name\">\n"
"                , onder vermelding van factuurnummer <span style=\"font-weight:bold;\" t-out=\"object.name or ''\">INV/2021/05/0005</span>.\n"
"            </t>\n"
"        </t>\n"
"        <br><br>\n"
"        Mocht u vragen hebben, aarzel dan niet om contact met ons op te nemen..\n"
"        <t t-if=\"not is_html_empty(object.invoice_user_id.signature)\">\n"
"            <br><br>\n"
"            <t t-out=\"object.invoice_user_id.signature or ''\"><br>Mitchell Admin</t>\n"
"        </t>\n"
"        <br><br>\n"
"        Met vriendelijke groet,\n"
"        <br>\n"
"        <t t-out=\"object.invoice_user_id.name or ''\">Mitchell Admin</t>\n"
"    </p>\n"
"</div>\n"
"            "

